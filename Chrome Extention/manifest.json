{
    "manifest_version": 3,
    "name": "Topic Organizer",
    "version": "1.0",
    "description": "Manage topics and their related links and texts.",
    "permissions": [
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  
  